{
  "name": "javascript",
  "version": "1.0.0",
  "description": "JavaScript design pattern",
  "scripts": {
    "lint": "npx eslint . --ext .json,.js --fix",
    "test": "npx jest --collect-coverage",
    "pretty": "npx prettier --no-config './**/*.{ts,js,json}' --write",
    "build:readme": "npx babel-node utils/readme/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patternsandbox/js.git"
  },
  "author": "Girma Nigusse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patternsandbox/js/issues"
  },
  "homepage": "https://github.com/patternsandbox/js#readme",
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/node": "7.14.5",
    "@babel/plugin-proposal-decorators": "7.14.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@babel/register": "7.14.5",
    "@babel/runtime": "7.14.6",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.3",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.2",
    "eslint": "7.28.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-json": "3.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "ts-jest": "27.0.3",
    "typescript": "4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty && lint-staged --allow-empty --verbose && npm run test"
    }
  },
  "lint-staged": {
    "./**/*.{js}": [
      "eslint . --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@patternsandbox/docs": "^1.0.12",
    "pino": "6.11.3",
    "pino-pretty": "5.0.2"
  }
}
